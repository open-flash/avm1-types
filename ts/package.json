{
  "name": "avm1-types",
  "version": "0.10.0",
  "homepage": "https://github.com/open-flash/avm1-types",
  "description": "Types for the Flash ActionScript Virtual Machine (AVM1)",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:open-flash/avm1-types.git"
  },
  "keywords": [
    "swf",
    "ast",
    "avm1"
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "https://spdx.org/licenses/MIT"
    },
    {
      "type": "Apache-2.0",
      "url": "https://spdx.org/licenses/Apache-2.0"
    }
  ],
  "scripts": {
    "build": "gulp lib:build",
    "watch": "gulp lib:watch",
    "test": "gulp test",
    "lint": "gulp lint",
    "format": "gulp format",
    "typedoc": "gulp lib:typedoc",
    "dist": "gulp lib:dist",
    "prepare": "gulp all:tsconfig.json && gulp tslint.json && gulp dist",
