    "semantic-types": "^0.1.1",
    "ts-tagged": "^1.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.3",
    "@types/gulp": "^4.0.6",
    "@types/minimist": "^1.2.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.2",
    "chai": "^4.2.0",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.2.0",
    "minimist": "^1.2.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "turbo-gulp": "^0.20.1",
    "typescript": "^3.6.3"
  },
  "c88": {
    "match": [
      "build/test/lib/*.{js,mjs}",
      "build/test/lib/**/*.{js,mjs}"
